#!/bin/bash

#----------------------------
# influx
#----------------------------
influxHost=192.168.4.55
influxPort=8086
influxDb=smokerWue
influxSenderId=wue-smoker

PILOGGER_SENSOR=$1
PILOGGER_DATA=$2

curl -i -XPOST "http://${influxHost}:${influxPort}/write?db=${influxDb}" --data-binary "sensor_data,sensor=${PILOGGER_SENSOR} value=${PILOGGER_DATA}" > /dev/null 2>&1

